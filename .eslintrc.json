{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "react-hooks"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": 0,
        "react/forbid-prop-types": 0,
        "jsx-a11y/anchor-is-valid": 2,
        "react-hooks/rules-of-hooks": "error",
        "react/jsx-filename-extension": "off",
        "react-hooks/exhaustive-deps": "off"
    }
}
